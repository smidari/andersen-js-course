{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console" : 0,
    "no-plusplus": "off",
    "import/prefer-default-export": "off",
    "no-use-before-define": "off"
  }
}